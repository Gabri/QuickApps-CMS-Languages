#: View/Elements/formatter_from.ctp:8
msgid "Link to file"
msgstr "Link a file"

#: View/Elements/formatter_from.ctp:9
msgid "Table of Files"
msgstr "Tabella dei file"

#: View/Elements/formatter_from.ctp:10
msgid "File URL"
msgstr "URL del file"

msgid "Files must be less than <b>%sB</b>."
msgstr "Il file deve essere meno di <b>%sB</b>."

msgid "Allowed file types: <b>%s</b>."
msgstr "Tipi di file permessi: <b>%s</b>."

msgid "Description"
msgstr "Descrizione"

msgid "Remove"
msgstr "Elimina"

msgid "Attachment"
msgstr "Allegato"

msgid "Size"
msgstr "Taglia"

msgid "Allowed extensions"
msgstr "Estensioni permesse"

msgid "Comma separated. i.e.: jpg,gif,png"
msgstr "Separati da virgola. es.: jpg,gif,png"

msgid "Number of files"
msgstr "Numero di file"

msgid "Maximum number of files users can upload for this field."
msgstr "Numero massimo di file che gli utenti possono caricare per questo campo."

msgid "Upload folder"
msgstr "Carica cartella"

msgid "Optional subdirectory within the upload destination where files will be stored. Do not include preceding or trailing slashes."
msgstr "Sottocartella opzionale dove verranno salvati i file caricati. Non mettere slash (/) all'inizio e alla fine."

msgid "Enable Description field"
msgstr "Attiva descrizione del campo"

msgid "The description field allows users to enter a description about the uploaded file."
msgstr "La descrizione del campo permette agli utenti di inserire una descrizione relativa al file caricato."

msgid "Invalid file extension."
msgstr "Estensione del file non valida."

msgid "You must upload at least one file."
msgstr "Devi caricare almeno un file."

